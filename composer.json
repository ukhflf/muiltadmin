{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "~2.3",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "^4.0",
        "freyo/flysystem-qcloud-cos-v5": "^1.2",
        "hyn/multi-tenant": "5.3.*",
        "iidestiny/laravel-filesystem-oss": "^1.1",
        "infyomlabs/adminlte-templates": "5.7.x-dev",
        "infyomlabs/laravel-generator": "5.7.x-dev",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7.0",
        "maatwebsite/excel": "^3.0",
        "overtrue/wechat": "~4.0",
        "overtrue/laravel-lang": "~3.0",
        "predis/predis": "^1.1",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.23",
        "symfony/dom-crawler": " ~3.1",
        "symfony/http-foundation": "~4.0",
        "workerman/gatewayclient": "^3.0",
        "yisonli/wxmenu": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/seeds/tenants"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
